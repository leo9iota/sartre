{
    "extends": "@sartre/ts/tsconfig.web.json",
    "compilerOptions": {
        "baseUrl": ".",
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitAny": false,
        "paths": {
            "@/*": ["./src/*"],
            "@/ui/*": ["./src/components/ui/*"],
            "@/common/*": ["./src/components/common/*"],
            "@/layout/*": ["./src/components/layout/*"],
            "@sartre/shared/*": ["../../packages/shared/*"],
        }
    }
}
